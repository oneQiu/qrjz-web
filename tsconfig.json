{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "removeComments": true,
    "target": "es5",
    "module": "commonjs",
    "allowJs": true,
    "checkJs": true,
    "jsx": "react",
    "sourceMap": true,
    "outDir": "./dist/",
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true 
  },
  "include": ["./src"]
}